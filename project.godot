; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Corrupscape"
config/description="Made with Godot Engine for the Pirate Software Game Jam 1/20/2024.

This is the first game that I've ever exported."
config/tags=PackedStringArray("game_jam_project", "work_on_now")
run/main_scene="res://Levels/MainMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Uncoded Studios/Corrupscape"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Assets/game_cover.png"
boot_splash/fullsize=false
config/icon="res://Assets/game_cover.png"
boot_splash/minimum_display_time=2000

[autoload]

GameManager="*res://ObjectScenes/GameManager.tscn"
tw="*res://addons/tw/tw.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=640
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/tw/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

